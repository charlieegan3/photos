<%= form_for(location, {action:"/admin/locations", method: "POST"}) { %>
  <div class="row">
    <div>
      <%= f.InputTag("Name") %>
    </div>
    <div>
      <%= f.InputTag("Latitude") %>
    </div>
    <div>
      <%= f.InputTag("Longitude") %>
    </div>
  </div>

  <br/>
  <%= f.SubmitTag("Create Location") %>
<% } %>
