<h1>Tags</h1>
<p><a href="./tags/new">New Tag</a></p>
<ul>
<%= for (tag) in tags { %>
  <%= if (!tag.Hidden) { %>
    <li>
      <a href="/admin/tags/<%= tag.Name %>">
      <%= tag.Name %>
      </a>
    </li>
  <% } %>
<% } %>
</ul>

<p>Hidden</p>
<ul>
<%= for (tag) in tags { %>
  <%= if (tag.Hidden) { %>
    <li>
      <a href="/admin/tags/<%= tag.Name %>">
      <%= tag.Name %>
      </a>
    </li>
  <% } %>
<% } %>
</ul>
