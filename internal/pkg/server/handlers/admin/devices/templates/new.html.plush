<h1>New Device</h1>

<%= form_for(device, {action:"/admin/devices", method: "POST"}) { %>
  <div class="mb1">
    <%= f.InputTag("Name") %>
  </div>
  <div class="mb1">
    <%= f.InputTag("ModelMatches") %>
  </div>
  <div class="mb2">
    <%= f.FileTag("Icon") %>
  </div>

  <%= f.SubmitTag("Create Device") %>
<% } %>
