<h1>New Location</h1>

<%= form_for(location, {action:"/admin/locations", method: "POST"}) { %>
  <div class="mb1">
    <%= f.InputTag("Name") %>
  </div>
  <div class="mb1">
    <%= f.InputTag("Latitude") %>
  </div>
  <div class="mb2">
    <%= f.InputTag("Longitude") %>
  </div>

  <%= f.SubmitTag("Create Location") %>
<% } %>
