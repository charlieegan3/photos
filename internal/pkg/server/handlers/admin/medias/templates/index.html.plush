<h1>Medias</h1>
<p><a href="./medias/new">New Media</a></p>
<p><%= len(medias) %> medias</p>
<ul>
<%= for (media) in medias { %>
  <li>
    <a href="/admin/medias/<%= media.ID %>">
      id: <%= media.ID %>
      <%= if (len(postMediaMap[media.ID]) > 0) { %>
      (<%= len(postMediaMap[media.ID]) %>)
      <% } %>
      <%= if (media.Kind != "jpg") { %>
       <%= media.Kind %>
      <% } %>
    </a>
  </li>
<% } %>
</ul>
